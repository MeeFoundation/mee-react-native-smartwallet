{
  "name": "mee_react_native_smartwallet",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android --active-arch-only",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "start-rc": "npx react-native start --reset-cache",
    "test": "jest",
    "manifest": "react-native-asset",
    "postinstall": "cd ios && pod install --repo-update",
    "android-release-aab": "cd android && ./gradlew bundleRelease",
    "android-release-test": "react-native run-android --variant release",
    "clean": "rm -rf ~/Library/Developer/Xcode/DerivedData && rm -rf node_modules/ /tmp/metro-*  && watchman watch-del-all && watchman shutdown-server && yarn cache clean",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "deep-clean": "rm -rf ~/Library/Developer/Xcode/DerivedData ios/build ios/Pods && cd android && ./gradlew clean && cd .. && rm -rf node_modules/ /tmp/metro-* && watchman watch-del-all && watchman shutdown-server && yarn cache clean && rm -rf yarn.lock",
    "reset": "yarn clean && yarn install",
    "deep-reset": "yarn deep-clean && yarn install"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5",
    "@react-native-community/blur": "^4.4.1",
    "@react-native-menu/menu": "^1.2.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.13",
    "@react-navigation/native-stack": "^7.1.14",
    "jotai": "^2.11.0",
    "lodash-es": "^4.17.21",
    "react": "18.3.1",
    "react-native": "0.76.5",
    "react-native-aes-crypto": "^3.2.1",
    "react-native-asset": "^2.1.1",
    "react-native-biometrics": "^3.0.1",
    "react-native-gesture-handler": "^2.21.2",
    "react-native-ios-context-menu": "3.x",
    "react-native-ios-utilities": "5.x",
    "react-native-mmkv": "^3.2.0",
    "react-native-reanimated": "^3.16.6",
    "react-native-safe-area-context": "^5.0.0",
    "react-native-screens": "^4.3.0",
    "react-native-svg": "^15.10.1",
    "react-native-svg-transformer": "^1.5.0",
    "zeego": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.76.5",
    "@react-native/eslint-config": "0.76.5",
    "@react-native/metro-config": "0.76.5",
    "@react-native/typescript-config": "0.76.5",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "prettier-plugin-organize-imports": "^4.1.0",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
