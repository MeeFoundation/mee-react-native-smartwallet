{
  "cSpell.words": ["Pressable"],
  "search.exclude": {
    "node_modules/": true,
    "vendor/": true,
    ".bundle/": true,
    "ios/": true,
    "android/": true,
    "__tests__/": true,
    "*.lock": true,
    ".*": true
  },
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "files.watcherExclude": {
    "**/target": true
  }
}
